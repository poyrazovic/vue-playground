{
  "name": "nuxt-playground",
  "version": "1.0.0",
  "description": "Learning NuxtJS",
  "author": "poyrazovic",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
